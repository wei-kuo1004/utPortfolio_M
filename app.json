{
  "expo": {
    "name": "UTLife",
    "slug": "utPortfolio_M",
    "version": "1.0.0",
    "scheme": "demo-app",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/icon/Icon-1024.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.utweikuo.utPortfolio_M"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1dea7f87-462c-4386-bd6c-69bbc8d44dae"
      }
    }
  }
}
